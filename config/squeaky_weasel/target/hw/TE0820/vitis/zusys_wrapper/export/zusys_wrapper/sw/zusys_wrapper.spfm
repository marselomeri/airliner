<?xml version="1.0" encoding="UTF-8"?>
<sdx:platform sdx:vendor="xilinx.com"
              sdx:library="sdx"
              sdx:name="zusys_wrapper"
              sdx:version="1.0"
              sdx:schemaVersion="1.0"
              xmlns:sdx="http://www.xilinx.com/sdx">
    <sdx:description>
zusys_wrapper
    </sdx:description>
    <sdx:systemConfigurations sdx:defaultConfiguration="zusys_wrapper">
        <sdx:configuration sdx:name="zusys_wrapper"
                           sdx:displayName="zusys_wrapper"
                           sdx:defaultProcessorGroup="standalone_domain"
                           sdx:runtimes="cpp">
            <sdx:description>zusys_wrapper</sdx:description>
            <sdx:bootImages sdx:default="standard">
                <sdx:image sdx:name="standard"
                            sdx:bif="zusys_wrapper/boot/zusys_wrapper.bif"
                            sdx:readme="zusys_wrapper/boot/generic.readme"
                            sdx:qemuArguments="zusys_wrapper/qemu/pmu_args.txt:zusys_wrapper/qemu/qemu_args.txt"
                            />
                <sdx:image sdx:name="standalone_domain"
                            sdx:bif="zusys_wrapper/boot/zusys_wrapper.bif"
                            sdx:readme="zusys_wrapper/boot/generic.readme"
                            sdx:qemuArguments="zusys_wrapper/standalone_domain/qemu/pmu_args.txt:zusys_wrapper/standalone_domain/qemu/qemu_args.txt"
                            />
            </sdx:bootImages>
            <sdx:processorGroup sdx:name="standalone_domain"
                                sdx:displayName="standalone on psu_cortexa53_0"
                                sdx:cpuType="cortex-a53"
                                sdx:cpuInstance="psu_cortexa53_0">
                <sdx:os sdx:name="standalone"
                        sdx:displayName="standalone"
                        sdx:bspLibraryPaths="zusys_wrapper/standalone_domain/bsplib/lib"
                        sdx:bspIncludePaths="zusys_wrapper/standalone_domain/bspinclude/include"
                        sdx:bspConfig="zusys_wrapper/standalone_domain/system.mss"
                />
            </sdx:processorGroup>
        </sdx:configuration>
    </sdx:systemConfigurations>
</sdx:platform>
