              { value: 8, name:'SENSOR_ORIENTATION_ROLL_180'},
              { value: 9, name:'SENSOR_ORIENTATION_ROLL_180_YAW_45'},
              { value: 10, name:'SENSOR_ORIENTATION_ROLL_180_YAW_90'},
              { value: 11, name:'SENSOR_ORIENTATION_ROLL_180_YAW_135'},
              { value: 12, name:'SENSOR_ORIENTATION_PITCH_180'},
              { value: 13, name:'SENSOR_ORIENTATION_ROLL_180_YAW_225'},
