.row.os-theme-dark
  article.col-xs-12.col-sm-12
    header
      span.widget-icon
        i.fa.fa-table
      h2 ULR - Command and Data Handling
    div.widget-body.table-responsive
      table.table.table-condensed
        tbody
          tr
            th Cmd Count
            td
              div(data-cdr={tlm:[{name:'/ULR/ULR_HkTlm_t/usCmdCnt'}],indicator:'text'}) ---
          tr
            th Error Count
            td
              div(data-cdr={tlm:[{name:'/ULR/ULR_HkTlm_t/usCmdErrCnt'}],indicator:'text'}) ---
          tr
            th Min Distance
            td
              div(data-cdr={tlm:[{name:'/ULR/ULR_HkTlm_t/MinDistance', format:'%.6f'}],indicator:'text'}) ---
          tr
            th Max Distance
            td
              div(data-cdr={tlm:[{name:'/ULR/ULR_HkTlm_t/MaxDistance'}],indicator:'text'}) ---
          tr
            th Current Distance
            td
              div(data-cdr={tlm:[{name:'/ULR/ULR_HkTlm_t/CurrentDistance', format:'%.6f'}],indicator:'text'}) ---
          tr
            th Covariance
            td
              div(data-cdr={tlm:[{name:'/ULR/ULR_HkTlm_t/Covariance', format:'%.6f'}],indicator:'text'}) ---
          tr
            th Sensor Type
            td
              div(data-cdr={tlm:[{name:'/ULR/ULR_HkTlm_t/Type', calibration:{ type:'enumeration',
              enumerations:[
              { value: 0, name:'LASER'},
              { value: 1, name:'ULTRASOUND'},
              { value: 2, name:'INFRARED'},
              { value: 3, name:'RADAR'}
              ]}}],indicator:'text'}) ---
          tr
            th Sensor ID
            td
              div(data-cdr={tlm:[{name:'/ULR/ULR_HkTlm_t/SensorID'}],indicator:'text'}) ---
          tr
            th Sensor Orientation
            td
              div(data-cdr={tlm:[{name:'/ULR/ULR_HkTlm_t/SensorOrientation', calibration:{ type:'enumeration',
              enumerations:[
